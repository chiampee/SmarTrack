{
  "name": "cursor-8-7",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build:extension": "vite build --config vite.extension.config.ts && cp extension/manifest.json dist-extension/ && cp extension/options.html dist-extension/ && cp extension/options.js dist-extension/ && cp -r extension/icons dist-extension/ && cp dist-extension/extension/popup.html dist-extension/popup.html && sed -i '' 's|src=\"../popup.js\"|src=\"./popup.js\"|g' dist-extension/popup.html",
    "test:extension": "echo 'Extension testing - use test-extension.html'",
    "db:reset": "echo 'Database reset - clear browser IndexedDB manually'",
    "setup": "./scripts/install.sh",
    "setup:mac": "./scripts/install-mac.sh",
    "start:mac": "./scripts/start-mac.sh",
    "start:windows": "scripts\\start-windows.bat",
    "test:install": "./scripts/test-installation.sh",
    "test:errors": "node scripts/test-error-handling.js",
    "test:windows": "scripts\\test-windows-installation.bat",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "e2e": "playwright test",
    "configure": "node scripts/setup.js",
    "setup:env": "./scripts/setup-env.sh",
    "test:chat": "node scripts/test-chat-functionality.js",
    "test:db": "node scripts/quick-db-test.js",
    "test:db-ops": "node scripts/test-db-operations.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.585.0",
    "@headlessui/react": "^2.2.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vercel/node": "^5.3.6",
    "autoprefixer": "^10",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "lucide-react": "^0.525.0",
    "openai": "^4.19.1",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^7.6.3",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "vitest": "^3.2.4",
    "zustand": "^5.0.6"
  },
